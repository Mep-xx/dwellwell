{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "types": ["vite/client", "node"],
    "paths": {
      "@shared/*": ["../shared/*"],
      "@types/*": ["../shared/types"],
      "@/*": ["src/*"]
    },
    "jsx": "react-jsx",
    "typeRoots": ["./node_modules/@types", "./src/types"]
  },
  "include": ["src", "src/types"]
}
